#: ../src/interface.c:452
msgid ""
"%A - Artist\n"
"%L - Album\n"
"%N - Track number (2-digit)\n"
"%Y - Year (4-digit or \"0\")\n"
"%T - Song title"
msgstr ""
"%A - 艺术家\n"
"%L - 专辑\n"
"%N - 曲目编号 (2 位)\n"
"%Y - 年代 (4 位或“0”)\n"
"%T - 歌曲名称"

#: ../src/interface.c:457
#, c-format
msgid "%G - Genre"
msgstr "%G - 类型"

