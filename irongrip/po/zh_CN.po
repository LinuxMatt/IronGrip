msgid "Playlist file: "
msgstr "播放列表文件："

#: ../src/interface.c:486 ../src/prefs.c:661 ../src/prefs.c:671
msgid "Music file: "
msgstr "音乐文件："

#: ../src/interface.c:500
msgid ""
"This is relative to the destination folder (from the General tab).\n"
"Can be blank.\n"
"Default: %A - %L\n"
"Other example: %A/%L"
msgstr ""
"相对于目标文件夹 (来自通用菜单)。\n"
"可留空白。\n"
"默认：%A - %L\n"
"其它示例：%A/%L"

#: ../src/interface.c:512
msgid ""
"This will be stored in the album directory.\n"
"Can be blank.\n"
"Default: %A - %L"
msgstr ""
"这将会保存到专辑目录下。\n"
"可留空白。\n"
"默认：%A - %L"

#: ../src/interface.c:523
msgid ""
"This will be stored in the album directory.\n"
"Cannot be blank.\n"
"Default: %A - %T\n"
"Other example: %N - %T"
msgstr ""
"这将会保存到专辑目录下。\n"
"不能空白。\n"
"默认： %A - %T\n"
"其它示例：%N - %T"

